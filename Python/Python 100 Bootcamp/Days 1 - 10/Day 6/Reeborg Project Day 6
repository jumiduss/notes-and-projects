# World Info Python
# Maze
def turn_right():
    turn_left()
    turn_left()
    turn_left()
    
def right_hand_rule():
    if not right_is_clear():
        if front_is_clear():
            move()
        else:
            turn_left()
    else:
        turn_right()
        move()
while not at_goal():
    right_hand_rule()